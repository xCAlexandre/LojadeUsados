@page "/TS/Perfil/Perfil"
@using System.Net.Http
@inject HttpClient Http
@inject NavigationManager NavigationManager

<div class="text-left">
    
</div>
<div class="content">
    <div class="card">
        <figure class="text-left" style="margin: 50px">           
                 <h1 class="display-4">Perfil</h1> 
        </figure>
        <div class="row">
            <div class="col-lg-12">
                <div class="table-responsive table-space">
                    <table class="table table-hover" id="table-lista-cadastrados">
                        <thead>
                            <div class="container">
                                <form class="row g-3 needs-validation" novalidate>
                                    <div class="row">
                                        <div class="col-6">
                                            <div class="text-center">
                                                <img src="data:image/png;base64,"
                                                    class="rounded-circle" style="height: 125px;" ;"  alt="">
                                            <div class="d-grid gap-2 col-6 mx-auto" >
                                                <button type="button" class="btn btn-success btn-sm" style="margin-top: 50px; margin-bottom: 15px;">Change picture</button>
                                            </div>
                                            <div class="d-grid gap-2 col-6 mx-auto">
                                                <button type="button" class="btn btn-success btn-sm" style="margin-top: 15px;">Status</button>
                                            </div>
                                            <div class="d-grid gap-2 col-6 mx-auto">
                                                <button type="button" class="btn btn-success btn-sm" style="margin-top: 15px;">historic</button>
                                            </div>
                                            </div>
                                            
                                        </div>
                                        <div class="col-6">
                                            <label for="Name" class="form-label">Name</label>
                                            <h3>@usuario.Nome</h3>

                                            <label for="Address" class="form-label">Address</label>
                                            <h3>@usuario.Endereco</h3>

                                            <label for="CPF" class="form-label">CPF</label>
                                            <h3>@usuario.Cpf</h3>

                                            <label for="birthday" class="form-label">birthday</label>
                                            <h3>@usuario.DatadeNasc</h3>

                                            <label for="E-mail" class="form-label">E-mail</label>
                                            <h3>@usuario.email</h3>
                                            
                                            <label for="Phone number" class="form-label">Phone number</label>
                                            <h3>@usuario.telefone</h3>
                                        </div>
                                    </div>      
                                </form>
                            </div>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
@code{
    Usuario usuario = new Usuario();
    protected override async Task OnInitializedAsync()
    {
    Console.WriteLine("teste2");
    usuario = await Http.GetFromJsonAsync<Usuario>($"/Usuario/GetById?id={1}");
    Console.WriteLine(usuario.Nome);
    }
    

} 
